<?xml version="1.0" encoding="utf-8"?>
<com.thelumiereguy.neumorphicview.views.NeumorphicConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ad"
    app:enable_preview="true"
    app:layout_cardRadius="100"
    app:layout_enableHighlight="true"
    app:layout_enableShadow="true"
    app:layout_highlightColor="#FFFFFF"
    app:layout_highlightDx="-3dp"
    app:layout_highlightDy="-3dp"
    app:layout_highlightRadius="3dp"
    app:layout_neu_backgroundColor="#EEEEEE"
    app:layout_shadowColor="#000000"
    app:layout_shadowDx="3dp"
    app:layout_shadowDy="3dp"
    app:layout_shadowRadius="3dp"
    tools:context=".log_in">

    <ImageView
        android:id="@+id/iv_l_bgwhite"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.3"
        android:src="@color/white"
        android:contentDescription="TODO" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.145" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.19" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.27" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.36" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.63" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.72" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.81" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.855" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguide_l_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.075" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.200" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.260" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.290" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.350" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.380" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.480" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.540" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.60" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.66" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.72" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.81" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguide_l_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.84" />

    <TextView
        android:id="@+id/tv_l_appname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aquatico"
        android:shadowColor="#80000000"
        android:shadowDx="3"
        android:shadowDy="3"
        android:shadowRadius="2"
        android:text="@string/app_name"
        android:textColor="#303030"
        android:textSize="@dimen/heading"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_10"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_1"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_1" />

    <TextView
        android:id="@+id/tv_l_appname2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aquatico"
        android:shadowColor="#70FFFFFF"
        android:shadowDx="-3"
        android:shadowDy="-3"
        android:shadowRadius="2"
        android:text="@string/app_name"
        android:textColor="#303030"
        android:textSize="@dimen/heading"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_10"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_1"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_1" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/til_l_email"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        app:boxBackgroundMode="outline"
        app:boxStrokeColor="@color/white"
        app:boxStrokeErrorColor="@color/white"
        app:errorEnabled="true"
        app:errorTextColor="@color/primarydark"
        app:hintEnabled="false"
        app:layout_cardRadius="100"
        app:layout_neu_backgroundColor="#EEEEEE"
        app:layout_enableHighlight="true"
        app:layout_enableShadow="true"
        app:layout_highlightColor="#80FFFFFF"
        app:layout_highlightDx="-1dp"
        app:layout_highlightDy="-1dp"
        app:layout_highlightRadius="3dp"
        app:layout_shadowColor="#000000"
        app:layout_shadowDx="2dp"
        app:layout_shadowDy="2dp"
        app:layout_shadowRadius="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/hguide_l_3"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_10"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_1"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_2">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/tiet_l_email"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/transparent"
            android:hint="Email ID"
            android:inputType="textEmailAddress"
            android:paddingLeft="@dimen/text"
            android:paddingRight="@dimen/text"
            android:textColor="#303030"
            android:textColorHint="#50303030"
            android:textSize="@dimen/text" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/til_l_pwd"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        app:boxBackgroundMode="outline"
        app:boxStrokeColor="@color/white"
        app:boxStrokeErrorColor="@color/white"
        app:boxStrokeWidth="0dp"
        app:errorEnabled="true"
        app:errorTextColor="@color/primarydark"
        app:hintEnabled="false"
        app:layout_cardRadius="100"
        app:layout_neu_backgroundColor="#EEEEEE"
        app:layout_enableHighlight="true"
        app:layout_enableShadow="true"
        app:layout_highlightColor="#80FFFFFF"
        app:layout_highlightDx="-1dp"
        app:layout_highlightDy="-1dp"
        app:layout_highlightRadius="3dp"
        app:layout_shadowColor="#000000"
        app:layout_shadowDx="2dp"
        app:layout_shadowDy="2dp"
        app:layout_shadowRadius="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/hguide_l_5"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_10"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_1"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_4"
        app:passwordToggleEnabled="false"
        app:passwordToggleTint="#FFFFFF">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/tiet_l_pwd"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/transparent"
            android:hint="Password"
            android:inputType="textPassword"
            android:paddingLeft="@dimen/text"
            android:paddingRight="@dimen/text"
            android:textColor="#303030"
            android:textColorHint="#50303030"
            android:textSize="@dimen/text" />
    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/b_l_login"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/button_gradient"
        android:text="LOG IN"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/text"
        app:layout_constraintBottom_toBottomOf="@+id/hguide_l_8"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_7"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_4"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_7"
        />

    <TextView
        android:id="@+id/tv_l_fpwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:linksClickable="true"
        android:paddingStart="@dimen/text"
        android:text="Forgot Your Password"
        android:textColor="#303030"
        android:textSize="@dimen/extra"
        android:paddingTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_1"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_5" />

    <ImageView
        android:id="@+id/iv_l_bar"
        android:layout_width="0dp"
        android:layout_height="3dp"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_5"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_1"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_9"
        app:srcCompat="@drawable/_black" />

    <ImageView
        android:id="@+id/iv_l_bar2"
        android:layout_width="0dp"
        android:layout_height="3dp"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_10"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_6"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_9"
        app:srcCompat="@drawable/_black" />

    <TextView
        android:id="@+id/tv_l_or"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OR"
        android:textColor="#303030"
        android:textSize="@dimen/extra"
        app:layout_constraintBottom_toBottomOf="@+id/hguide_l_10"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_6"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_5"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_8" />

    <Button
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/textbox_white"
        android:drawableLeft="@drawable/google"
        android:fontFamily="@font/roboto_medium"
        android:paddingLeft="20dp"
        android:text="Sign in with Google"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#BB303030"
        android:textSize="@dimen/text"
        app:layout_constraintBottom_toTopOf="@+id/hguide_l_11"
        app:layout_constraintEnd_toStartOf="@+id/vguide_l_9"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_2"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_10" />

    
    <TextView
        android:id="@+id/tv_l_tc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="By Continuing, You Agree Appoindone's"
        android:textColor="#303030"
        android:textSize="@dimen/extra"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_10"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_1"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_14" />

    <TextView
        android:id="@+id/ltv_l_tc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:linksClickable="true"
        android:text="Terms and Condition"
        android:textColor="#303030"
        android:textSize="@dimen/extra"
        app:layout_constraintEnd_toEndOf="@+id/vguide_l_7"
        app:layout_constraintStart_toStartOf="@+id/vguide_l_4"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_tc" />

    <TextView
        android:id="@+id/tv_l_softui1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_cardRadius="100"
        app:layout_enableHighlight="true"
        app:layout_enableShadow="true"
        app:layout_highlightColor="#40FFFFFF"
        app:layout_highlightDx="-1dp"
        app:layout_highlightDy="-1dp"
        app:layout_highlightRadius="3dp"
        app:layout_neu_backgroundColor="@color/white"
        app:layout_shadowColor="#000000"
        app:layout_shadowDx="2dp"
        app:layout_shadowDy="2dp"
        app:layout_shadowRadius="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/hguide_l_8"
        app:layout_constraintEnd_toEndOf="@id/vguide_l_7"
        app:layout_constraintStart_toStartOf="@id/vguide_l_4"
        app:layout_constraintTop_toTopOf="@+id/hguide_l_7"/>

</com.thelumiereguy.neumorphicview.views.NeumorphicConstraintLayout>